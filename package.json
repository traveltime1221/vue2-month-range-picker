{
  "name": "vue2-month-range-picker",
  "version": "1.0,0",
  "description": "這是一個非常簡易而且只有選擇月份範圍的 Vue2 組件，支持更換月份格式。",
  "author": "traveltime1221",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/traveltime1221/vue2-month-range-picker.git"
  },
  "keywords": [
    "vue2",
    "vue-component",
    "month-range-picker",
    "date-range-picker"
  ],
  "bugs": {
    "url": "https://github.com/traveltime1221/vue2-month-range-picker/issues"
  },
  "homepage": "https://github.com/traveltime1221/vue2-month-range-picker#readme",
  "type": "module",
  "main": "dist/vue2-month-range-picker.umd.js",
  "module": "dist/vue2-month-range-picker.esm.js",
  "exports": {
    ".": {
      "import": "./dist/vue2-month-range-picker.esm.js",
      "require": "./dist/vue2-month-range-picker.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^1.0.3",
    "rollup": "^4.30.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": ">=2.6.0 <2.7.0",
    "vue-template-compiler": ">=2.6.0 <2.7.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
